import operator

def solution(N, stages):
    answer = []
    stages.sort()
    
    #실패율 구하는거 다시하기
    failureRate = {}
    for i in range(1,N+1):
        rate = 0
        for j in stages:
            stage = len(stages)
            if i == j:
                rate += 1
            failureRate[i] = rate/stage
        stages.remove(j)
        print(stages)
    print(failureRate)
     
    #내림차순 정렬
    sfailureRate= sorted(failureRate.items(), key=operator.itemgetter(1), reverse=True)
    print(sfailureRate)
    
    for i in sfailureRate:
        answer.append(i[0])
    return answer
