import operator

def solution(N, stages):
    answer = []
    stages.sort()
    
#실패율
    failureRate = {}
    for i in range(1,N+1):
        rate = 0
        for j in range(len(stages)):
            if i == stages[j]:
                rate += 1
            if rate == 0:
                failureRate[i] = 0
            else:
                failureRate[i] = rate/k
        k -= rate
     
    #내림차순 정렬
    sfailureRate= sorted(failureRate.items(), key=operator.itemgetter(1), reverse=True)
    print(sfailureRate)
    
    for i in sfailureRate:
        answer.append(i[0])
    return answer
