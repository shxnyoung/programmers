def solution(array, commands):
    answer = []
    
    for a in range(len(commands)):
        i = commands[a][0]-1
        j = commands[a][1]
        slicedArray = array[i:j]
        slicedArray.sort()
        # print(slicedArray)
        k = commands[a][2] - 1
        # print(slicedArray[k])
        answer.append(slicedArray[k]) 
        
        
        
    return answer
